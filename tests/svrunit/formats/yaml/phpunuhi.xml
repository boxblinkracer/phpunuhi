<phpunuhi
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../config.xsd"
>

    <translations>

        <set name="yaml">
            <format>
                <yaml indent="2" sort="true"/>
            </format>
            <locales>
                <locale name="de">./de.yaml</locale>
                <locale name="en">./en.yaml</locale>
                <locale name="nl">./nl.yaml</locale>
            </locales>

        </set>

    </translations>

</phpunuhi>
