{
    "name": "boxblinkracer/phpunuhi",
    "version": "1.4.0",
    "license": "MIT",
    "description": "Easy tool to work with translation files for validation, exports, imports and more.",
    "homepage": "https://github.com/boxblinkracer/phpunuhi",
    "keywords": [
        "translation",
        "deepl",
        "open-ai",
        "shopware"
    ],
    "authors": [
        {
            "name": "Christian Dangl"
        }
    ],
    "bin": [
        "bin/phpunuhi"
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-simplexml": "*",
        "deeplcom/deepl-php": "^1.2",
        "doctrine/dbal": "^3.5",
        "google/cloud-translate": "^1.12",
        "orhanerday/open-ai": "^3.4",
        "symfony/console": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "nunomaduro/phpinsights": "^1.14",
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
