{
    "name": "boxblinkracer/phpunuhi",
    "version": "1.1.0",
    "license": "MIT",
    "description": "Easy tool to work with translation files for validation, exports, imports and more.",
    "homepage": "https://github.com/boxblinkracer/phpunuhi",
    "keywords": [
        "translation",
        "deepl",
        "open-ai",
        "shopware"
    ],
    "authors": [
        {
            "name": "Christian Dangl"
        }
    ],
    "bin": [
        "bin/phpunuhi"
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "deeplcom/deepl-php": "^1.2",
        "doctrine/dbal": "^3.5",
        "google/cloud-translate": "^1.12",
        "orhanerday/open-ai": "^3.4",
        "symfony/console": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
